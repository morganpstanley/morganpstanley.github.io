        "message": {
          "type": "text",
          "required": true
        }
      },
      "actions": [
        {
          "type": "email",
          "to": "morganpstanley@gmail.com"
        }
      ]
    }
  },
  "devDependencies": {
    "@types/react": "^17.0.37",
